{
  "name": "rsclone",
  "version": "0.0.1",
  "description": "MoneyKeeper(MKeeper) - приложение для контроля и учета доходов и расходов.",
  "main": "src/index.ts",
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igormotorin/rsclone/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igormotorin/rsclone.git"
  },
  "homepage": "https://github.com/igormotorin/rsclone#readme",
  "scripts": {
    "dev": "webpack serve --color --hot --mode=development",
    "prod": "webpack serve --color --hot --mode=production",
    "clean": "rm -f -r build",
    "buildapp": "webpack --mode=production",
    "build": "pnpm clean && pnpm buildapp",
    "analyze": "webpack --analyze --mode=production",
    "prepare": "husky install",
    "prettier": "prettier --check \"**/*.+(js|cjs|ts)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|cjs|ts)\"",
    "lint": "eslint **/*.{js,mjs,cjs,ts,mts,cts}",
    "lint:fix": "eslint **/*.{js,mjs,cjs,ts,mts,cts,} --fix",
    "lint:fix-staged": "eslint --fix",
    "lint:css": "stylelint src",
    "lint:css:fix": "pnpm lint:css -- --fix",
    "lint:css:fix-staged": "pnpm stylelint -- --fix",
    "lint-all": "pnpm prettier && pnpm lint && pnpm lint:css",
    "fix-all": "pnpm prettier:fix && pnpm lint:fix && pnpm lint:css:fix",
    "fix-staged": "lint-staged",
    "svgo": "cross-env svgo $FILE",
    "svgo-all": "svgo -r -f src"
  },
  "devDependencies": {
    "@alexey-koran/prettier-config": "^0.0.2",
    "@alexey-koran/tsconfig": "^0.0.2",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/node": "^18.11.18",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.1",
    "esbuild-loader": "^2.21.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "globals": "^13.20.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss": "^8.4.21",
    "postcss-assets": "^6.0.0",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "stylelint-config-clean-order": "^2.3.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.1",
    "stylelint-prettier": "^2.0.0",
    "svgo": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "chart.js": "3.9.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1"
  }
}
